<h1>New Owners Page</h1>

<%= form_for @owner2 do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :street %>
  <%= f.text_field :street %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br>
  <%= f.submit "Create Acccount"%>
<% end %>
